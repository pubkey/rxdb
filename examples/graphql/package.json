{
  "name": "graphql",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "(cd ../../ && npx yarn@1.22.22 pack ../../ --filename ./examples/graphql/rxdb-local.tgz)",
    "dev": "webpack serve",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "node ./server/index.js",
    "refresh": "rimraf -r node_modules/rxdb/ && npm i ",
    "build": "webpack",
    "build:serve": "ws -p 8888 -d dist/",
    "test:e2e": "testcafe chromium -e test/ --hostname localhost",
    "test:pouchdb": "STORAGE=pouchdb concurrently \"npm run server\" \"npm run build:serve\" \"sleep 10 && npm run test:e2e\" --kill-others --success first",
    "test:dexie": "STORAGE=dexie concurrently \"npm run server\" \"npm run build:serve\" \"sleep 10 && npm run test:e2e\" --kill-others --success first"
  },
  "browser": {
    "fs": false
  },
  "private": true,
  "author": "pubkey",
  "license": "MIT",
  "type": "module",
  "//": "NOTICE: For the Github CI we use the local RxDB build (rxdb-local.tgz). In your app should just install 'rxdb' from npm instead",
  "dependencies": {
    "rxdb": "file:rxdb-local.tgz",
    "@apollo/server": "4.11.3",
    "concurrently": "9.1.2",
    "cors": "2.8.5",
    "express-graphql": "0.12.0",
    "graphql-client": "2.0.1",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.16.2",
    "local-web-server": "5.4.0",
    "normalize.css": "8.0.1",
    "pouchdb-adapter-idb": "9.0.0",
    "rxjs": "7.8.1",
    "ws": "8.16.0"
  },
  "devDependencies": {
    "async-test-util": "2.5.0",
    "css-loader": "7.1.2",
    "eslint": "8.57.1",
    "esm": "3.2.25",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.3",
    "mini-css-extract-plugin": "2.9.2",
    "ncp": "2.0.0",
    "rimraf": "5.0.10",
    "source-map-loader": "5.0.0",
    "style-loader": "4.0.0",
    "testcafe": "3.7.1",
    "testcafe-hammerhead": "31.7.4",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0"
  }
}
