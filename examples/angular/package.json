{
  "name": "angular",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npm run preinstall:rxdb && npm run preinstall:versions",
    "preinstall:rxdb": "(cd ../../ && npx yarn@1.22.10 pack ../../ --filename ./examples/angular/rxdb-local.tgz)",
    "preinstall:versions": "node ./scripts/ensure-equal-versions.js",
    "postinstall": "ngcc",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "ts-node ./src/server.ts",
    "ng": "ng",
    "type": "module",
    "dev": "ng serve",
    "build": "npm run build:frontend && npm run build:copy && npm run build:server && echo \"BUILD SUCCESS\"",
    "build:frontend": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configuration production --aot",
    "build:server": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng run angular:server:production",
    "build:serve": "ws -p 8888 -d dist/angular/browser",
    "build:serve9000": "ws -p 9000 -d dist/angular/",
    "build:copy": "cp test/multitab.html dist/angular/browser/multitab.html",
    "test": "testcafe chromium -e test/e2e.test.js --hostname localhost --selector-timeout 30000",
    "test:ci": "testcafe chrome:headless -e test/e2e.test.js --hostname localhost --selector-timeout 30000",
    "test:build": "concurrently \"npm run build:serve\" \"npm run test:ci\" --kill-others --success first",
    "// test:ssr": "TODO use npm run serve:ssr instead when bug is fixed",
    "test:ssr": "concurrently \"npm run server\" \"npm run dev:ssr\" \"sleep 20 && ts-mocha  --bail --exit ./test/ssr.test.ts\" --kill-others --success first",
    "lint": "tslint app/src/**/*.ts",
    "analyze": "webpack-bundle-analyzer dist/angular/stats-es2015.json",
    "dev:ssr": "ng run angular:serve-ssr",
    "serve:ssr": "node dist/angular/server/main.js",
    "build:ssr": "ng build --configuration production && ng run angular:server:production",
    "prerender": "ng run angular:prerender"
  },
  "private": true,
  "author": "pubkey",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "12.2.16",
    "@angular/cdk": "12.2.13",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/platform-server": "12.2.16",
    "@angular/pwa": "12.2.16",
    "@angular/router": "12.2.16",
    "@angular/service-worker": "12.2.16",
    "@nguniversal/express-engine": "12.1.3",
    "assert": "2.0.0",
    "express": "4.17.3",
    "express-pouchdb": "4.2.0",
    "font-awesome": "4.7.0",
    "mocha": "9.0.2",
    "pouchdb-adapter-http": "7.2.2",
    "pouchdb-adapter-idb": "7.2.2",
    "roboto-npm-webfont": "1.0.1",
    "rxdb": "file:rxdb-local.tgz",
    "rxjs": "7.5.4",
    "stream": "0.0.2",
    "tslib": "2.3.1",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.16",
    "@angular/cli": "12.2.16",
    "@angular/compiler-cli": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@nguniversal/builders": "13.0.2",
    "@types/express": "4.17.13",
    "@types/mocha": "8.2.3",
    "@types/node": "14.18.12",
    "@types/pouchdb-adapter-http": "6.1.3",
    "@types/pouchdb-adapter-idb": "6.1.4",
    "@types/pouchdb-adapter-memory": "6.1.3",
    "async-test-util": "2.0.0",
    "concurrently": "7.0.0",
    "got": "11.8.3",
    "local-web-server": "5.1.1",
    "pouchdb-server": "4.2.0",
    "testcafe": "1.18.4",
    "ts-mocha": "8.0.0",
    "ts-node": "10.7.0",
    "tslint": "6.1.3",
    "typescript": "4.5.5",
    "webpack-bundle-analyzer": "4.5.0"
  }
}