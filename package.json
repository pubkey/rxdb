    "modifyjs": "0.3.1",
    "object-path": "0.11.4",
    "pouchdb-all-dbs": "1.0.2",
    "pouchdb-core": "7.1.1",
    "pouchdb-find": "7.0.0",
    "pouchdb-replication": "7.1.1",
