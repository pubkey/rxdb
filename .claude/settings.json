{
    "buildCommand": "npm run build",
    "testCommand": "npm run test:fast:memory",
    "lintCommand": "npm run lint",
    "permissions": {
        "allow": [
            "Bash(npm run lint*)",
            "Bash(npm run test*)"
        ],
        "deny": [
            "Write(plugins/**)",
            "Edit(plugins/**)",
            "Delete(plugins/**)"
        ]
    }
}